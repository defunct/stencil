<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <servlet>
    <servlet-name>Stencil</servlet-name>
    <servlet-class>com.agtrz.stencil.StencilServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Stencil</servlet-name>
    <url-pattern>/stencil/</url-pattern>
  </servlet-mapping>
  <welcome-file-list> 
    <welcome-file>index.html</welcome-file> 
  </welcome-file-list> 
</web-app>

